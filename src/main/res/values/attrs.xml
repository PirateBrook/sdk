<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FrameLayout">
        <attr name="measureAllChildren" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="FrameLayout_layout">
        <attr name="layout_gravity" format="integer"/>
    </declare-styleable>
    <declare-styleable name="UrlImageView">
        <attr name="src" format="reference"/>
        <attr name="baselineAlignBottom" format="boolean"/>
        <attr name="baseline" format="dimension"/>
        <attr name="adjustViewBounds" format="boolean"/>
        <attr name="maxWidth" format="dimension"/>
        <attr name="maxHeight" format="dimension"/>
        <attr name="scaleType" format="integer"/>
        <attr name="tint" format="reference"/>
        <attr name="drawAlpha" format="integer"/>
        <attr name="cropToPadding" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="PullHeaderLayout">
        <attr name="header_height" format="dimension"/>
        <attr name="header_expand_height" format="dimension"/>
        <attr name="header_shrink_height" format="dimension"/>
        <attr name="header" format="reference"/>
        <attr name="content" format="reference"/>
        <attr name="action" format="reference"/>
    </declare-styleable>

</resources>